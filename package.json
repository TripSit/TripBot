{
  "name": "tripbot",
  "version": "6.6.0",
  "description": "A bot for TripSit (and maybe the world)",
  "main": "start.ts",
  "prisma": {
    "schema": "src/prisma/tripbot/schema.prisma",
    "seed": "ts-node ./src/prisma/seed.ts"
  },
  "engines": {
    "node": "v21.4.0",
    "npm": "10.2.4"
  },
  "scripts": {
    "## TRIPBOT ##": "",
    "tripbot:start": "docker compose --project-name tripbot up -d --force-recreate --build tripbot ",
    "tripbot:logs": "docker container logs tripbot -f -n 100",
    "tripbot:test": "docker exec -it tripbot npx jest --silent -c ./src/jest/jest.config.ts ",
    "tripbot:test1": "npx jest -c ./src/jest/jest.config.ts",
    "tripbot:lint": "docker exec -it tripbot npx eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts . --cache",
    "tripbot:deployCommands": "docker exec -it tripbot ts-node --transpile-only ./src/discord/utils/commandDeploy.ts -- deployCommands",
    "## DATABASE ##": "",
    "db:start": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_database",
    "db:logs": "docker container logs tripbot_database -f -n 100",
    "db:migrate": "docker exec -it tripbot npx prisma migrate deploy",
    "db:generate": "npx prisma generate && npx prisma generate --schema src/prisma/moodle/schema.prisma && docker exec -it tripbot npx prisma generate && docker exec -it tripbot npx prisma generate --schema src/prisma/moodle/schema.prisma",
    "db:seed": "docker exec -it tripbot npx prisma db seed",
    "db:formatSchema": "docker exec -it tripbot npx prisma format",
    "db:validateSchema": "docker exec -it tripbot npx prisma validate",
    "db:pushDevChanges": "docker exec -it tripbot npx prisma db push && npm run db:generate",
    "db:createMigrationFile": "docker exec -it tripbot npx prisma migrate dev -n member_fix",
    "## PGADMIN ##": "",
    "pgadmin": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_pgadmin",
    "pgadmin:logs": "docker container logs tripbot_pgadmin -f -n 100",
    "## CODESPACE ##": "",
    "codespace:start": "npx tsc-watch --onSuccess 'npx nodemon --config ./nodemon.json'",
    "codespace:deploy": "npx ts-node --transpile-only ./src/discord/utils/commandDeploy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TripSit/TripBot"
  },
  "keywords": [
    "Harm",
    "Reduction",
    "Bot"
  ],
  "author": "Eric / Moonbear / Teknos <eric@tripsit.me>",
  "contributors": [
    "Hipperooni",
    "Ajar",
    "Change"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TripSit/TripBot/issues"
  },
  "homepage": "https://github.com/TripSit/TripBot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "@google/generative-ai": "^0.2.1",
    "@napi-rs/canvas": "^0.1.51",
    "@octokit/types": "^12.6.0",
    "@octokit/webhooks-types": "^7.3.2",
    "@prisma/client": "^5.10.2",
    "@sentry/node": "^7.106.0",
    "argon2": "^0.40.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "common-tags": "^1.8.2",
    "convert-units": "^2.3.4",
    "cross-fetch": "^4.0.0",
    "discord-api-types": "^0.37.73",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "fuse.js": "^7.0.0",
    "graphql-request": "^6.1.0",
    "he": "^1.2.0",
    "helmet": "^7.1.0",
    "imdb-api": "^4.4.1",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "natural": "^6.12.0",
    "node-os-utils": "^1.3.7",
    "objection": "^3.1.4",
    "octokit": "^3.1.2",
    "openai": "^4.28.4",
    "rollbar": "^2.26.3",
    "rss-parser": "^3.13.0",
    "source-map-support": "^0.5.21",
    "stopword": "^3.0.1",
    "telegraf": "^4.16.3",
    "tripsit_drug_db": "github:tripsit/drugs",
    "trivia-api": "^1.0.1",
    "underscore": "^1.13.6",
    "winston": "^3.12.0",
    "winston-transport-rollbar-3": "^3.2.6",
    "wordlist-english": "^1.2.1",
    "youtube-search-without-api-key": "^2.0.1"
