{
  "name": "tripbot",
  "version": "6.6.0",
  "description": "A bot for TripSit (and maybe the world)",
  "main": "start.ts",
  "prisma": {
    "schema": "src/prisma/tripbot/schema.prisma",
    "seed": "ts-node ./src/prisma/seed.ts"
  },
  "engines": {
    "node": ">=v22.14.0",
    "npm": ">=v10.9.2"
  },
  "scripts": {
    "## TRIPBOT ##": "",
    "tripbot:dev": "npm run tripbot:start && npm run tripbot:logs",
    "tripbot:start": "tmux new-session -d -s tripbot \"npx tsc-watch --onSuccess 'node ./build/src/start.js'\"",
    "tripbot:logs": "tmux attach -t tripbot",
    "tripbot:stop": "tmux kill-session -t tripbot || true",
    "tripbot:restart": "npm run tripbot:stop && npm run tripbot:start && npm run tripbot:logs",
    "tripbot:test": "npx jest -c ./src/jest/jest.config.ts",
    "tripbot:lint": "npx eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts . --cache",
    "tripbot:deployCommands": "ts-node --transpile-only ./src/discord/utils/commandDeploy.ts -- deployCommands",
    "## DATABASE ##": "",
    "db:start": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_database",
    "db:logs": "docker container logs tripbot_database -f -n 100",
    "db:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate && npx prisma generate --schema src/prisma/moodle/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:formatSchema": "npx prisma format",
    "db:validateSchema": "npx prisma validate",
    "db:pushDev": "npx prisma db push && npm run db:generate",
    "db:migrateDev": "npx prisma migrate dev -n CHANGE_ME",
    "## PGADMIN ##": "",
    "pgadmin": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_pgadmin",
    "pgadmin:logs": "docker container logs tripbot_pgadmin -f -n 100",
    "prepare": "husky",
    "lint-staged": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TripSit/TripBot"
  },
  "keywords": [
    "Harm",
    "Reduction",
    "Bot"
  ],
  "author": "Eric / Moonbear / Teknos <eric@tripsit.me>",
  "contributors": [
    "Hipperooni",
    "Ajar",
    "Change",
    "theimperious1"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TripSit/TripBot/issues"
  },
  "homepage": "https://github.com/TripSit/TripBot#readme",
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@google/generative-ai": "^0.24.1",
    "@keycloak/keycloak-admin-client": "^26.3.1",
    "@napi-rs/canvas": "^0.1.74",
    "@octokit/types": "^14.1.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@prisma/client": "^6.12.0",
    "@sentry/node": "^9.40.0",
    "argon2": "^0.43.1",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "common-tags": "^1.8.2",
    "convert-units": "^2.3.4",
    "cross-fetch": "^4.1.0",
    "discord-api-types": "^0.38.16",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "fuse.js": "^7.1.0",
    "graphql-request": "^7.2.0",
    "he": "^1.2.0",
    "helmet": "^8.1.0",
    "husky": "^9.1.7",
    "imdb-api": "^4.4.1",
    "luxon": "^3.7.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "node-os-utils": "^1.3.7",
    "objection": "^3.1.5",
    "octokit": "^5.0.3",
    "openai": "^5.10.2",
    "rollbar": "^2.26.4",
    "rss-parser": "^3.13.0",
    "source-map-support": "^0.5.21",
    "telegraf": "^4.16.3",
    "tripsit_drug_db": "github:tripsit/drugs",
    "underscore": "^1.13.7",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0",
    "winston-transport-rollbar-3": "^3.2.6",
    "youtube-search-without-api-key": "^2.0.7"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
