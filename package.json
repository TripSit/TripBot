{
  "name": "tripbot",
  "version": "6.6.0",
  "description": "A bot for TripSit (and maybe the world)",
  "main": "start.ts",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "engines": {
    "node": "v20.5.0",
    "npm": "9.8.0"
  },
  "scripts": {
    "## START ## ": "",
    "tripbot": "docker compose --project-name tripbot up -d --force-recreate --build tripbot ",
    "postgres": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_database",
    "prisma": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_prisma",
    "pgadmin": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_pgadmin",
    "api": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_api",
    "all": "docker compose --project-name tripbot up -d --force-recreate --build",
    "## TRIPBOT ##": "",
    "tripbot:logs": "docker container logs tripbot -f -n 100",
    "tripbot:test": "docker exec -it tripbot npx jest --silent -c ./src/jest/jest.unit.config.ts",
    "tripbot:lint": "docker exec -it tripbot npx eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts . --cache",
    "tripbot:deploy": "docker exec -it tripbot ts-node --transpile-only ./src/discord/utils/commandDeploy.ts",
    "tripbot:dbPull": "docker exec -it tripbot npx prisma db pull && npx prisma generate",
    "## PRISMA ##": "",
    "prisma:pull": "docker exec -it tripbot_prisma npx prisma db pull",
    "prisma:generate": "docker exec -it tripbot_prisma npx prisma generate",
    "prisma:migrate": "docker exec -it tripbot_prisma npx prisma migrate dev",
    "prisma:validate": "docker exec -it tripbot_prisma npx prisma validate",
    "prisma:format": "docker exec -it tripbot_prisma npx prisma format",
    "## KNEX ##": "",
    "knex:migrate:make": "docker exec -it tripbot_prisma npx knex --knexfile ./src/knex/knexfile.ts migrate:make appeals",
    "knex:migrate:latest": "docker exec -it tripbot_prisma npx knex --knexfile ./src/knex/knexfile.ts migrate:latest",
    "knex:migrate:down": "docker exec -it tripbot_prisma npx knex --knexfile ./src/knex/knexfile.ts migrate:down",
    "knex:migrate:up": "docker exec -it tripbot_prisma npx knex --knexfile ./src/knex/knexfile.ts migrate:up",
    "knex:migrate:full": "docker exec -it tripbot_prisma npm run knex",
    "# API #": "",
    "api:start": "rimraf dist && swc ./src -d ./dist && node --inspect=0.0.0.0 dist/index.js",
    "api:logs": "docker logs tripbot_api -f -n 100",
    "api:test": "jest --silent -c ./src/testing/jest.unit.config.ts --detectOpenHandles",
    "api:lint": "npx eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts . --cache",
    "## CODESPACE ##": "",
    "codespace:start": "npx tsc-watch --onSuccess 'npx nodemon --config ./nodemon.json'",
    "codespace:deploy": "npx ts-node --transpile-only ./src/discord/utils/commandDeploy.ts",
    "## PRISMA-START ##": "",
    "knex": "npm run migrate && npm run pull && npm run generate && npm run define && npm run lint && npm run studio",
    "migrate": "npx knex --knexfile ./src/knex/knexfile.ts migrate:latest",
    "pull": "npx prisma db pull",
    "generate": "npx prisma generate",
    "define": "npx ts-node --transpile-only ./src/global/utils/genDBDefs.ts",
    "lint": "npx eslint --fix ./src/global/@types/database.d.ts --cache",
    "studio": "npx prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TripSit/TripBot"
  },
  "keywords": [
    "Harm",
    "Reduction",
    "Bot"
  ],
  "author": "Eric / Moonbear / Teknos <eric@tripsit.me>",
  "contributors": [
    "Ajar",
    "Change",
    "Hipperooni",
    "SevenCats"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TripSit/TripBot/issues"
  },
  "homepage": "https://github.com/TripSit/TripBot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "@napi-rs/canvas": "^0.1.43",
    "@octokit/types": "^11.1.0",
    "@octokit/webhooks-types": "^7.2.0",
    "@prisma/client": "^5.1.1",
    "@sentry/node": "^7.64.0",
    "argon2": "^0.31.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "common-tags": "^1.8.2",
    "convert-units": "^2.3.4",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "databank": "^1.0.9",
    "databank-redis": "^0.18.0",
    "discord-api-types": "^0.37.54",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "fuse.js": "^6.6.2",
    "he": "^1.2.0",
    "helmet": "^7.0.0",
    "imdb-api": "^4.4.1",
    "keycloak-admin": "^1.14.22",
    "knex": "^2.5.1",
    "knex-types": "^0.5.0",
    "luxon": "^3.4.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "node-os-utils": "^1.3.7",
    "objection": "^3.1.1",
    "octokit": "^3.1.0",
    "openai": "^4.0.1",
    "pg": "^8.11.3",
    "rss-parser": "^3.13.0",
    "sonar-scanner": "^3.1.0",
    "source-map-support": "^0.5.21",
    "trivia-api": "^1.0.1",
    "underscore": "^1.13.6",
    "winston": "^3.10.0",
    "youtube-search": "^1.1.6",
    "youtube-search-without-api-key": "^1.1.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/convert-units": "^2.3.6",
    "@types/cors": "^2.8.13",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/he": "^1.2.0",
    "@types/jest": "^29.5.3",
    "@types/luxon": "^3.3.1",
    "@types/morgan": "^1.9.4",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.5.1",
    "@types/node-os-utils": "^1.3.1",
    "@types/source-map-support": "^0.5.6",
    "@types/supertest": "^2.0.12",
    "@types/underscore": "^1.11.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-sonarjs": "^0.20.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "telegraf": "^4.12.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
