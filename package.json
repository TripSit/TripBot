{
  "name": "tripbot",
  "version": "6.6.0",
  "description": "A bot for TripSit (and maybe the world)",
  "main": "start.ts",
  "prisma": {
    "schema": "src/prisma/tripbot/schema.prisma"
  },
  "engines": {
    "node": "v21.4.0",
    "npm": "10.2.4"
  },
  "scripts": {
    "## TRIPBOT ##": "",
    "tripbot:start": "docker compose --project-name tripbot up -d --force-recreate --build tripbot ",
    "tripbot:logs": "docker container logs tripbot -f -n 100",
    "tripbot:test": "docker exec -it tripbot npx jest --silent -c ./src/jest/jest.config.ts ",
    "tripbot:test1": "npx jest -c ./src/jest/jest.config.ts",
    "tripbot:lint": "docker exec -it tripbot npx eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts . --cache",
    "tripbot:deployCommands": "docker exec -it tripbot ts-node --transpile-only ./src/discord/utils/commandDeploy.ts",
    "tripbot:db:generate": "npx prisma generate && npx prisma generate --schema src/prisma/moodle/schema.prisma && docker exec -it tripbot npx prisma generate && docker exec -it tripbot npx prisma generate --schema src/prisma/moodle/schema.prisma",
    "## DATABASE ##": "",
    "db:start": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_database",
    "db:logs": "docker container logs tripbot_database -f -n 100",
    "db:formatSchema": "docker exec -it tripbot npx prisma format",
    "db:validateSchema": "docker exec -it tripbot npx prisma validate",
    "db:generateClient": "npx prisma generate && docker exec -it tripbot npx prisma generate",
    "db:pushDev": "docker exec -it tripbot npx prisma db push && npx prisma generate",
    "db:migrateDev": "docker exec -it tripbot npx prisma migrate dev -n betterImageLogging",
    "## PGADMIN ##": "",
    "pgadmin": "docker compose --project-name tripbot up -d --force-recreate --build tripbot_pgadmin",
    "pgadmin:logs": "docker container logs tripbot_pgadmin -f -n 100",
    "## CODESPACE ##": "",
    "codespace:start": "npx tsc-watch --onSuccess 'npx nodemon --config ./nodemon.json'",
    "codespace:deploy": "npx ts-node --transpile-only ./src/discord/utils/commandDeploy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TripSit/TripBot"
  },
  "keywords": [
    "Harm",
    "Reduction",
    "Bot"
  ],
  "author": "Eric / Moonbear / Teknos <eric@tripsit.me>",
  "contributors": [
    "Ajar",
    "Change",
    "Hipperooni",
    "SevenCats"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TripSit/TripBot/issues"
  },
  "homepage": "https://github.com/TripSit/TripBot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "@napi-rs/canvas": "^0.1.44",
    "@octokit/types": "^12.4.0",
    "@octokit/webhooks-types": "^7.3.1",
    "@prisma/client": "^5.7.0",
    "argon2": "^0.31.2",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "common-tags": "^1.8.2",
    "convert-units": "^2.3.4",
    "cross-fetch": "^4.0.0",
    "discord-api-types": "^0.37.65",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fuse.js": "^7.0.0",
    "graphql-request": "^6.1.0",
    "he": "^1.2.0",
    "helmet": "^7.1.0",
    "imdb-api": "^4.4.1",
    "knex": "^3.0.1",
    "knex-types": "^0.5.0",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "node-os-utils": "^1.3.7",
    "objection": "^3.1.3",
    "octokit": "^3.1.2",
    "openai": "^4.20.1",
    "rss-parser": "^3.13.0",
    "source-map-support": "^0.5.21",
    "telegraf": "^4.15.3",
    "tripsit_drug_db": "github:tripsit/drugs",
    "trivia-api": "1.0.0",
    "underscore": "^1.13.6",
    "winston": "^3.11.0",
    "youtube-search-without-api-key": "^2.0.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/convert-units": "^2.3.9",
    "@types/express": "^4.17.21",
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.3.7",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.10.3",
    "@types/node-os-utils": "^1.3.4",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^2.0.16",
    "@types/underscore": "^1.11.15",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.2"
  }
}
